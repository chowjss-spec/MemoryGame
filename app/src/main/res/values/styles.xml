<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="buttonStyle">
        <item name="android:layout_width">90dp</item>
        <item name="android:layout_height">90dp</item>
        <item name="android:src">@drawable/x</item>
        <item name="android:background">@drawable/imageview_grey_border</item>
    </style>
    <style name="buttonStyle2">
        <item name="android:layout_width">132dp</item>
        <item name="android:layout_height">132dp</item>
<!--        <item name="android:src">@drawable/x</item>-->
        <item name="android:background">@drawable/imageview_grey_border</item>
    </style>
    <style name="SplashTheme" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/splash</item>
    </style>
    <style name="tick">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">#00000000</item>
        <item name="android:src">@drawable/tick</item>
        <item name="android:layout_gravity">top</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
    <style name="buttonStyle3">
        <item name="android:layout_width">90dp</item>
        <item name="android:layout_height">90dp</item>
        <item name="android:src">@drawable/sushi</item>
        <item name="android:background">@drawable/imageview_blue_border</item>
    </style>
    <style name="buttonStyle4">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">120dp</item>
        <item name="android:src">@drawable/sushi3</item>
        <item name="android:background">@drawable/imageview_blue_border1</item>
    </style>
    <style name="buttonStyle5">
        <item name="android:layout_width">90dp</item>
        <item name="android:layout_height">90dp</item>
        <item name="android:src">@drawable/sushi2</item>
        <item name="android:background">@drawable/imageview_blue_border</item>
    </style>
    <style name="buttonStyle6">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">120dp</item>
        <item name="android:src">@drawable/sushi2</item>
        <item name="android:background">@drawable/imageview_blue_border</item>
    </style>
    <style name="buttonStyle7">
        <item name="android:text">@string/playButton</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black1</item>
        <item name="fontFamily">Courier New Tur</item>
        <item name="android:background">@drawable/sushibtn</item>
    </style>
    <style name="buttonStyle8">
        <item name="android:text">@string/completeButton</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black1</item>
        <item name="fontFamily">Courier New Tur</item>
        <item name="android:background">@drawable/sushibtn</item>
